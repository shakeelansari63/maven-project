pipeline {
  agent any
  stages {
    stage('Initialize'){
      steps {
        echo 'Hello there. This is initializing the testing process.'
     }
    }
    stage('Build'){
      steps {
        echo 'Now Building...'
     }
    }
    stage('Deploy'){
      steps {
        echo 'Now Deploying...'
     }
    }
  }
}
